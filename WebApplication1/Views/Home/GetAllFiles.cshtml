@model List<string>

@{
    Layout = null;
}

<ul class="list-group">
    @if (Model.Count > 0)
    {
        foreach (var item in Model)
        {
            <li class="list-group-item">@item</li>
        }
    }
    else
    {
        <li class="list-group-item">-- Empty Folder --</li>
    }

</ul>
